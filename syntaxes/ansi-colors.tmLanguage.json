        "1": { "name": "punctuation.definition.tag.file.path.ansi" },
        "2": { "name": "variable.language.file.name.ansi" },
        "3": { "name": "constant.numeric.file.line.ansi" }
      }
    },
    {
      "name": "file.link.without.line.number",
      "match": "(file://.*/)([^/\\s:]+)(?=\\s|$)",
